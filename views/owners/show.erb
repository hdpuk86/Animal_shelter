<nav class="nav--second">
  <button class="button button--second"><a href="/owners/<%=@owner.id%>/edit">Edit <%=@owner.name%></a></button>
  <button class="button button--second"><a href="/owners/<%=@owner.id%>/delete">Delete <%=@owner.name%></a></button>
</nav>

<h1><%=@owner.name()%></h1>
<article>
  <div class="text">
    <p><%=@owner.name()%> is looking for...</p>
    <br>
    <p>Type of pet: <%=@owner.type%></p>
    <p>Preferred sex of pet: <%=@owner.preferred_sex%></p>
    <p>Child friendly pet: <%=@owner.children%></p>
  </div>
</article>
<br/>
<div class="matches">
  <article id="matchesbox">
    <h2>Possible Matches</h2>
    <br/>
    <div class="matches">
    <%matches = @owner.find_matches%>
    <%for match in matches%>
    <article class="box">
      <a href="/pets/<%=match.id%>"/><h2><%=match.name%></h2><img class ="petPhoto" src="<%=match.image%>" alt="picture of pet"></a>
    </article>
    <%end%>
  </div>
  </article>
</div>
