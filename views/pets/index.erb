<h1>PETS</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Type</th>
    <th>Breed</th>
    <th>Admission Date</th>
    <th>Status</th>
  </tr>
  <%for pet in @pets%>
  <tr>
    <td><%=pet.name%></td>
    <td><%=pet.type%></td>
    <td><%=pet.breed%></td>
    <td><%=pet.admission_date%></td>
    <td><%=pet.status%></td>
  </tr>
  <%end%>
</table>
